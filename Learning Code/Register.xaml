<Window x:Class="Learning_Code.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="500" Width="500">
    <Grid Height="480" Width="580">
        <Label Content="Registration form" FontSize="15" FontWeight="SemiBold" Foreground="red" 
               Height="35" HorizontalAlignment="Left" 
               HorizontalContentAlignment="Center"
               Name="label2" VerticalAlignment="Top" Width="246" Margin="126,28,0,0" />
        <Label Content="First name" VerticalAlignment="Top" Margin="45,97,0,0" Width="64" HorizontalAlignment="Left" />
        <TextBox BorderBrush="#FF1A0202" Height="23" HorizontalAlignment="Left" Margin="126,146,0,0" Name="FirstNameTextBox" VerticalAlignment="Top" Width="246"/>
        <Label Content="Last name" VerticalAlignment="Top" Margin="45,146,0,0" Width="64" HorizontalAlignment="Left" />
        <TextBox BorderBrush="#FF1A0202" Height="23" HorizontalAlignment="Left" Margin="126,97,0,0" x:Name="LAstNameTextBox" VerticalAlignment="Top" Width="246"/>
        <TextBox BorderBrush="#FF1A0202" Height="23" HorizontalAlignment="Left" Margin="126,191,0,0" x:Name="OtherNameTextBox" VerticalAlignment="Top" Width="246"/>
        <Label Content="Email ID" VerticalAlignment="Top" Margin="45,191,0,0" Width="64" HorizontalAlignment="Left" />


    </Grid>
</Window>
